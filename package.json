{
  "name": "universal-react",
  "version": "0.0.1",
  "author": "Guido Schmitz <hello@guidoschmitz.co>",
  "description": "Universal React boilerplate with Redux and more to quickly bootstrap your next React project.",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "dev": "babel-node devServer.js",
    "server": "babel-node source/server/index.js",
    "test": "babel-node source/test/index.js",
    "lint": "eslint source",
    "watch": "watch 'clear && npm run lint -s && npm run test -s' source",
    "check": "npm run lint && npm run test && npm outdated --depth=0"
  },
  "dependencies": {
    "express": "4.14.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.5.1",
    "react-router-redux": "4.0.5",
    "react-simple-universal": "0.4.2",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-simple-router": "2.0.4",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "chai": "3.5.0",
    "enzyme": "2.3.0",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.10.0",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "5.2.2",
    "mocha": "2.5.3",
    "react-addons-test-utils": "15.1.0",
    "rimraf": "2.5.0",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "live",
    "edit",
    "webpack",
    "babel"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:guidsen/universal-react.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guidsen/universal-react/issues"
  },
  "homepage": "https://github.com/guidsen/universal-react"
}
